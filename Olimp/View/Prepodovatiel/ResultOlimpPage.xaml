<Page x:Class="Olimp.View.Prepodovatiel.ResultOlimpPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Olimp.View.Prepodovatiel"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="ResultOlimpPage">

    <Grid Background="Wheat">
        <ComboBox x:Name="Protocol_name" Width="200" Height="30" VerticalAlignment="Top"    HorizontalAlignment="Center" IsTextSearchEnabled="True" IsEditable="True" Margin="0,175,0,0" />
        <ComboBox x:Name="Student_name"  Width="200" Height="30" VerticalAlignment="Center" HorizontalAlignment="Center" IsTextSearchEnabled="True" IsEditable="True" DisplayMemberPath="FullName" />
        
        <TextBox x:Name="score_tete" Width="200" Height="30" Margin="300,245,300,175"/>
        
        
        <TextBox x:Name="resultt" Width="200" Height="30" Margin="300,280,300,140"/>
        
        <Button Width="200" Height="30" Margin="300,315,300,105" Click="SaveNewResult" Content="Сохранить"/>

    </Grid>
</Page>
