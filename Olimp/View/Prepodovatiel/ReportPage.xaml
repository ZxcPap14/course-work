<Page x:Class="Olimp.View.Prepodovatiel.ReportPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Olimp.View.Prepodovatiel"
      mc:Ignorable="d" 
      d:DesignHeight="800" d:DesignWidth="1300"
      Title="ReportPage">

    <Grid >

        <Button Background="#3498DB" Width="100" Height="30" HorizontalAlignment="Left" VerticalAlignment="Top" Click="Back" Content="Назад"/>

        <TextBox Background="#8B9FB4" x:Name="Names" Width="200" Height="30" Margin="0,195,0,0" HorizontalAlignment="Center" VerticalAlignment="Top"/>
        <Button Background="#3498DB" Width="200" Height="30" Margin="0,240,0,180" Click="GenerateReport" Content="Создать отчет" HorizontalAlignment="Center"/>
    </Grid>
</Page>
